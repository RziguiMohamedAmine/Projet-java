            <TextField fx:id="tfCode" prefHeight="39.0" prefWidth="200.0" promptText="verification code" styleClass="tf_box" />
         </children>
      </HBox>
      <HBox layoutX="10.0" layoutY="145.0" prefHeight="39.0" prefWidth="252.0" styleClass="tf_box">
         <children>
            <TextField fx:id="tfNewPass" prefHeight="48.0" prefWidth="251.0" promptText="New Password" styleClass="tf_box" />
         </children>
      </HBox>
      <Button layoutX="469.0" layoutY="225.0" mnemonicParsing="false" onAction="#OnNewPass" prefHeight="31.0" prefWidth="67.0" styleClass="blue-btn" text="OK" />
