<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="5.0" style="-fx-background-radius: 7;" stylesheets="@../public/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pkg3a11javaproj.OrderItemVBoxController">
   <children>
      <ImageView fx:id="image" fitHeight="100.0" pickOnBounds="true" preserveRatio="true" />
      <VBox alignment="CENTER" spacing="5.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="name" text="Name">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Label>
            <Label fx:id="quantity" text="Quantity" />
            <Label fx:id="price" text="Price" />
         </children>
      </VBox>
   </children>
   <styleClass>
      <String fx:value="white-background" />
      <String fx:value="shadow" />
   </styleClass>
</VBox>
